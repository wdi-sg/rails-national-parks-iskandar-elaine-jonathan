<h1>All Parks</h1>

<% @parks.each do |park| %>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%= park.name%></h3>
  </div>
  <div class="panel-body">
    <a href="/parks/<%= park.id %>"><img src="<%= park.picture %>" alt=""></a>
    <br>
  </div>
</div>
<% end %>
